{
  "manifest_version": 2,
  "name": "visma-online-test-data",
  "version": "1.1.1",

  "description": "Click the button to generate test data for various Visma Online pages",
  "homepage_url": "https://github.com/samiljan/testdata",

  "permissions": [
    "activeTab",
    "storage"
  ],

  "background": {
    "scripts": ["browser-polyfill.js", "background.js"]
  },

  "browser_action": {
    "default_icon": "icons/logo-32.png",
    "default_title": "Visma Online test data"
  },

  "applications": {
    "gecko": {
      "id": "testdata@samiljan.github.io",
      "update_url": "https://samiljan.github.io/testdata/updates.json"
    }
  },

  "options_ui": {
    "page": "options/options.html"
  }
}